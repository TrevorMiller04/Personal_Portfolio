{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.py"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/supabase-test",
      "dest": "/api/supabase-test.py"
    }
  ]
}